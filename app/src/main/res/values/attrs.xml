<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TaoBaoView">
        <attr name="ringProgressColor" format="color" />
        <attr name="ringWidth" format="dimension" />
        <attr name="ringImage" format="reference" />
        <attr name="ringMax" format="integer" />
    </declare-styleable>

    <declare-styleable name="PercentImageView">
        <attr name="piv_basics" format="enum">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
        <attr name="piv_percent" format="float" />
    </declare-styleable>
</resources>